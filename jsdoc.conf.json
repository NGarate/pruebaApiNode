{
  "source": {
    "includePattern": ".js$",
    "exclude": ["./node_modules", "./docs" ,"./coverage"],
    "include": ["./", "./package.json", "./readme.md"]
  },
  "sourceType": "module",
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "./node_modules/ink-docstrap/template",
    "destination": "docs/",
    "encoding": "utf8",
    "recurse": true,
    "verbose": false
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false,
    "systemName": "Prueba NodeJS API",
    "footer": "",
    "copyright":  "ngarate",
    "includeDate": true,
    "navType": "vertical",
    "theme": "paper",
    "linenums": true,
    "collapseSymbols": true,
    "inverseNav": true,
    "outputSourceFiles": true ,
    "dateFormat": "DD/MM/YYYY",
    "syntaxTheme": "dark",
    "sort": true,
    "search": true 
  }
}
