{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "includePattern": ".js$",
    "exclude": ["./node_modules", "./docs"],
    "include": ["./", "./package.json", "./readme.md"]
  },
  "sourceType": "module",
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "./node_modules/docdash",
    "destination": "./docs/",
    "encoding": "utf8",
    "recurse": true,
    "verbose": false
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false,
    "useLongnameInNav": true,
    "showInheritedInNav": true,
    "default": {
      "outputSourceFiles": true,
      "includeDate": true
    }
  },
  "docdash": {
    "static": [true],
    "sort": [true],
    "sectionOrder": [
         "Classes",
         "Modules",
         "Externals",
         "Events",
         "Namespaces",
         "Mixins",
         "Tutorials",
         "Interfaces"
    ],
    "search": [true],
    "collapse": [true],
    "wrap": [false],
    "typedefs": [true],
    "navLevel": [1],
    "private": [false],
    "removeQuotes": ["trim"]
  }
}
